{
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start-debug": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" npx expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "jest",
    "test-update": "jest -u",
    "eject": "expo eject",
    "surrender": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules && yarn && yarn cache clean"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@react-native-community/masked-view": "0.1.5",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.19",
    "apollo-upload-client": "^12.1.0",
    "expo": "~36.0.0",
    "expo-font": "~8.0.0",
    "formik": "^2.1.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "native-base": "^2.13.8",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-hook-form": "^5.1.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-masked-text": "^1.13.0",
    "react-native-paper": "^3.6.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^2.0.2",
    "react-navigation-tabs": "^2.6.2",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.0.0",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/jest": "^25.1.4",
    "@types/react": "~16.9.0",
    "@types/react-native": "~0.60.23",
    "@types/yup": "^0.26.33",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-preset-expo": "~8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest-expo": "^36.0.1",
    "prettier": "^2.0.2",
    "react-native-testing-library": "^1.13.0",
    "typescript": "~3.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ]
  }
}
